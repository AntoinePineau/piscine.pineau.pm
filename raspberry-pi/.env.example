# Configuration Pool Monitor pour Raspberry Pi
# Copier en .env ou configurer dans /etc/environment

# URL de l'API cloud (remplacer par votre vraie URL Vercel)
API_URL=https://pool-monitor-api.vercel.app/api/measurements

# Intervalle de mesure en secondes
MEASUREMENT_INTERVAL=30

# Niveau de log (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Timeout API en secondes
API_TIMEOUT=15

# Nombre de tentatives en cas d'Ã©chec
MAX_RETRIES=3