{
  "version": 2,
  "buildCommand": "echo 'No build needed - using static files'",
  "outputDirectory": "web-cloud",
  "cleanUrls": true,
  "trailingSlash": false,
  "functions": {
    "api-cloud/api/*.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api-cloud/api/$1"
    }
  ]
}